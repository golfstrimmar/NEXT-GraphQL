{
  "content": ".fildset-radio--preview{.form-check{min-height:30px;}input[type=\"radio\"]:checked,input[type=\"radio\"]:not(:checked){position:fixed!important;left:-9999px!important;height:0px;width:0px;}input[type=\"radio\"]+label{display:inline-block;position:absolute!important;left:30px!important;cursor:pointer;opacity:1;line-height:19px;transition:all 0.2s;color:#8c59df!important;&:hover{color:#290268!important;}}input[type=\"checkbox\"]:not(:checked)+label::after,input[type=\"checkbox\"]:checked+label::after{display:none!important;}input[type=\"radio\"]+label::before{content:\"\";position:absolute;left:-25px!important;top:50%!important;transform:translateY(-50%)!important;background-color:transparent!important;border-radius:50%;width:20px;height:20px;transition:all 0.2s;border:solid 2px #4a08b4;}input[type=\"radio\"]:checked+label>span::after,input[type=\"radio\"]:not(:checked)+label>span::after{display:none;}input[type=\"radio\"]:checked+label{font-weight:700;}input[type=\"radio\"]:checked+label>span,input[type=\"radio\"]:not(:checked)+label>span{content:\"\";position:absolute!important;width:14px;height:14px;border-radius:100%;top:50%;left:-22px;transform:translate(1%,-50%);border:solid 2px #4a08b4;background-color:#4a08b4;transition:all .3s;}input[type=\"radio\"]:checked+label>span{opacity:1;}input[type=\"radio\"]:not(:checked)+label>span{opacity:0;}}"
